{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.1.json",
	"author": "Seface Blocks",
	"name": "more-blocks",
	"packs": {
		"behaviorPack": "./core/behavior_pack",
		"resourcePack": "./core/resource_pack"
	},
	"regolith": {
		"dataPath": "./core/data",
		"filterDefinitions": {
			"csharp-compiler": {
				"url": "github.com/llgava/regolith-filters",
				"version": "0.0.5"
			},
			"json_cleaner": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.1.1"
			},
			"local_blocker": {
				"path": "./core/filters/local_blocker/Build/local_blocker.dll",
				"runWith": "dotnet"
			},
			"name_ninja": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.2.3"
			},
			"texture_list": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.1.2"
			}
		},
		"profiles": {
			"default": {
				"export": {
					"bpPath": "./build/BP_MB",
					"readOnly": true,
					"rpPath": "./build/RP_MB",
					"target": "exact"
				},
				"filters": [
					{
						"profile": "full_dev"
					}
				]
			},
			"dev": {
				"export": {
					"readOnly": false,
					"target": "development"
				},
				"filters": [
					{
						"filter": "csharp-compiler"
					},
					{
						"filter": "json_cleaner"
					},
					{
						"filter": "texture_list"
					},
					{
						"filter": "name_ninja",
						"settings": {
							"spawn_eggs": {
								"auto_name": false,
								"postfix": "",
								"prefix": ""
							}
						}
					}
				]
			}
		}
	}
}
